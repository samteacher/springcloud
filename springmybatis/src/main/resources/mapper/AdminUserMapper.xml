<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sam.springmybatis.dao.AdminUserMapper" >

	<select id="queryAdminUserAll"  resultType="AdminUser">
		SELECT
			id,
			email,
			account,
			forbidden
		FROM
			admin_user
	</select>
	
</mapper>
